extends layout

block content
    div#container
        h1 Thomas Belin
        h2 Developper @ 
            a(href='http://www.doyoubuzz.com', target='_blank') DoYouBuzz
        - for (var link in links)
            a(href=links[link]['url'], target='_blank', id=link, class='link', data-x=links[link]['position']['x'], data-y=links[link]['position']['y'], data-size=links[link]['size'])= link
